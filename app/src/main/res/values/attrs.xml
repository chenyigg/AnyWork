<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="InfoItem">
        <attr name="title" format="string"/>
        <attr name="info" format="string"/>
        <attr name="titlePic" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ExamPagerView">
        <attr name="sTitleCenterTextString" format="string"/>
        <attr name="sTitleCenterTextColor" format="color"/>
        <attr name="sTitleBackgroundColor" format="color"/>
        <attr name="sTitleRightIconResId" format="integer"/>
        <attr name="sLeftBottomTextString" format="string"/>
        <attr name="sLeftBottomTextColor" format="color"/>
        <attr name="sLeftBottomBackgroundColor" format="color"/>
        <attr name="sLeftBottomAlpha" format="float"/>
        <attr name="sRightBottomTextString" format="string"/>
        <attr name="sRightBottomTextColor" format="color"/>
        <attr name="sRightBottomBackgroundColor" format="color"/>
        <attr name="sRightBottomAlpha" format="float"/>
    </declare-styleable>


    <declare-styleable name="BaseDialog">
        <attr name="title" format="string"/>
        <attr name="titleSize" format="dimension"/>
        <attr name="titleColor" format="color"/>
        <attr name="cancle" format="boolean"/>
        <attr name="titleBackground" format="color|reference"/>
        <attr name="lineColor" format="color"/>
    </declare-styleable>

</resources>